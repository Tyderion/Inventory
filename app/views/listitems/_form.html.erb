<%= form_for @listitem do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :amount %><br />
    <%= f.number_field :amount %>
  </p>
  <p>
    <%= f.label :list_id %><br />
    <%= f.number_field :list_id %>
  </p>
  <p>
    <%= f.label :item_id %><br />
    <%= f.number_field :item_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
